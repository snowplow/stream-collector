collector {
  license { accept = true }
  interface = "0.0.0.0"
  port = ${PORT}

  streams {
     good {
       name = ${TOPIC_GOOD}
       googleProjectId = ${GOOGLE_PROJECT_ID}
       maxBytes = ${MAX_BYTES}
     }
     bad {
       name = ${TOPIC_BAD}
       googleProjectId = ${GOOGLE_PROJECT_ID}
       maxBytes = ${MAX_BYTES}
    }
  }

  networking {
    responseHeaderTimeout = 10 seconds
    bodyReadTimeout = 2 seconds
  }
}