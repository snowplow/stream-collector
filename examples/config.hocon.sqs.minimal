collector {
  interface = "0.0.0.0"
  port = 80

  streams {
    good = "{{good}}"
    bad = "{{bad}}"

    sink {
      enabled = sqs
      region = "{{region}}"
      threadPoolSize = 10

      aws {
        accessKey = iam
        secretKey = iam
      }

      backoffPolicy {
        minBackoff = 3000
        maxBackoff = 600000
      }
    }
  }
}