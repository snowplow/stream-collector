collector {
  interface = "0.0.0.0"
  port = 8080

  streams {
    good {
      name = "good"
      
      foo = "hello"
      bar = "world"
      
      buffer {
        byteLimit = 3145728
        recordLimit = 500
        timeLimit = 5000
      }
    }
    
    bad {
      name = "bad"
      
      foo = "hello"
      bar = "world"
      
      buffer {
        byteLimit = 3145728
        recordLimit = 500
        timeLimit = 5000
      }
    }    
  }

  ssl {
    enable = true
  }
}
